{
  "manifest_version": 2,
  "name": "Support Portal",
  "version": "2.1.0.10",
  "description": "Adds fixes and enhancements to the Support Portal",
  "browser_action": {
    "default_icon": "logo_19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo_19.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://test-support.sencha.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "supportportal.user.js"
  ],
  "update_url": "https://rawgit.com/tristanlee85/supportportal/master/extension/updates.xml"
}
